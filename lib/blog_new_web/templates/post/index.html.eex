<%= @valor %>
<ul class="collection with-header">
    <li class="collection-header">
        <h4>Todos os posts</h4>
    </li>
    <%= for post <- @posts do %>
    <li class="collection-item">
        <div><%= post.title %>
            <a href="#!" class="secondary-content">
                <i class="material-icons">send</i>
            </a>
        </div>
    </li>
    <%= end %>
</ul>
